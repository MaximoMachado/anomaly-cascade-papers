[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.3
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../rust/target/debug/libanomaly-cascade-gdextension.so"
linux.release.x86_64 =   "res://../rust/target/release/libanomaly-cascade-gdextension.so"
windows.debug.x86_64 =   "res://../rust/target/debug/anomaly-cascade-gdextension.dll"
windows.release.x86_64 = "res://../rust/target/release/anomaly-cascade-gdextension.dll"
macos.debug =            "res://../rust/target/debug/libanomaly-cascade-gdextension.dylib"
macos.release =          "res://../rust/target/release/libanomaly-cascade-gdextension.dylib"
macos.debug.arm64 =      "res://../rust/target/debug/libanomaly-cascade-gdextension.dylib"
macos.release.arm64 =    "res://../rust/target/release/libanomaly-cascade-gdextension.dylib"
